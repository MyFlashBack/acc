$tankkkLT=array(777777,19201,19985,19489,5937,17217,19009,58961);
$tankkkST=array(777777,13825,15617,19969,16897,21761,61697,12305,14609,14113,15905,55841,4145,63537,3649,15425,7249,3681,2417,5505,5265,2721,50849);
$tankkkTT=array(777777,46849,6145,7169,20993,22017,22273,58369,6929,9489,19473,58641,10785,14881,5425,6193,6209,16961,6225,15697,57937,3937,2433,3473,2977,2929);
$tankkkPT=array(777777,13569,20225,12049,19217,13089,13857,8497,13889,17729,9297,13905,15953,4737);
$tankkkSAU=array(777777,8705,9233,8481,11841,12369);
define('API_URL5', 'https://api.worldoftanks.ru/');
define('API_VERSION5', 'wot');
define('CLANINFO_METHOD5', API_URL5.API_VERSION5.'/tanks/stats/');
define('APPLICATION_ID5', '21d5c635a3a1fe0dee48aa41cf93a4e5');
define('ID5', $account1['account_id']);
$ID_AC5=$account1['account_id'];
$ch5 = curl_init();
curl_setopt($ch5, CURLOPT_URL, CLANINFO_METHOD5 . "?application_id=".APPLICATION_ID5."&account_id=".ID5);
curl_setopt($ch5, CURLOPT_RETURNTRANSFER, 1);
curl_setopt($ch5, CURLOPT_TIMEOUT, 60);
$outcome5 = json_decode(curl_exec($ch5), true);
if (curl_errno($ch5))
{
    echo "CURL returned error: ".curl_error($ch5)."\n";
    die();
}
curl_close($ch5);
if(empty($outcome5['status']) || $outcome5['status'] == 'error'){
    foreach($outcome5['error'] as $key5 => $value5){
        echo "  <b>{$key5}:</b> {$value5}<br/>";
    }
}else{
    foreach($outcome5['data'] as $account5) {
        for ($i = 0; $i <= 1000; $i++) {
            if(array_search ($account5[$i]['tank_id'], $tankkkLT)==true) {
                if($account5[$i]['tank_id']==$tank_100lt) {
                    if($account5[$i]['all']['battles']>30) {
                        $tank_100lt_Rvighil1 = round(($account5[$i]['all']['survived_battles'] / $account5[$i]['all']['battles'])*100, 0, PHP_ROUND_HALF_UP);
                        $tank_100lt_Ruron1 = round(($account5[$i]['all']['damage_dealt'] / $account5[$i]['all']['battles']), 0, PHP_ROUND_HALF_UP);
                        $tank_100lt_Rbatl1 = $account5[$i]['all']['battles'];
                        $tank_100lt_Rsvet1=round(($account5[$i]['all']['spotted'] / $account5[$i]['all']['battles']), 0, PHP_ROUND_HALF_UP);
                        $tank_100lt_Rvighil = $account5[$i]['all']['survived_battles'];
                        $tank_100lt_Ruron = $account5[$i]['all']['damage_dealt'];
                        $tank_100lt_Rbatl = $account5[$i]['all']['battles'];
                        $tank_100lt_Rsvet=$account5[$i]['all']['spotted'];
                    }
                    if($account5[$i]['stronghold_defense']['battles']>30) {
                        $tank_100lt_Nvighil = $account5[$i]['stronghold_defense']['survived_battles'];
                        $tank_100lt_Nuron = $account5[$i]['stronghold_defense']['damage_dealt'];
                        $tank_100lt_Nbatl = $account5[$i]['stronghold_defense']['battles'];
                        $tank_100lt_Nsvet = $account5[$i]['stronghold_defense']['spotted'];
                    }
                    if($account5[$i]['stronghold_skirmish']['battles']>30) {
                        $tank_100lt_Yvighil = $account5[$i]['stronghold_skirmish']['survived_battles'];
                        $tank_100lt_Yuron = $account5[$i]['stronghold_skirmish']['damage_dealt'] ;
                        $tank_100lt_Ybatl = $account5[$i]['stronghold_skirmish']['battles'];
                        $tank_100lt_Ysvet = $account5[$i]['stronghold_skirmish']['spotted'];
                    }
                    if($account5[$i]['stronghold_defense']['battles']>30 || $account5[$i]['stronghold_skirmish']['battles']>30) {
                        $tank_100lt_NYbatl = $tank_100lt_Nbatl + $tank_100lt_Ybatl;
                        $tank_100lt_NYvighil = round((($tank_100lt_Nvighil + $tank_100lt_Yvighil )/ $tank_100lt_NYbatl) * 100, 0, PHP_ROUND_HALF_UP);
                        $tank_100lt_NYuron = round((($tank_100lt_Nuron + $tank_100lt_Yuron )/ $tank_100lt_NYbatl), 0, PHP_ROUND_HALF_UP);
                        $tank_100lt_NYsvet = round((($tank_100lt_Nsvet + $tank_100lt_Ysvet )/ $tank_100lt_NYbatl), 0, PHP_ROUND_HALF_UP);
                    }
                }
                if($account5[$i]['tank_id']==$tank_rhm) {
                    if($account5[$i]['all']['battles']>30) {
                        $tank_rhm_Rvighil1 = round(($account5[$i]['all']['survived_battles'] / $account5[$i]['all']['battles'])*100, 0, PHP_ROUND_HALF_UP);
                        $tank_rhm_Ruron1 = round(($account5[$i]['all']['damage_dealt'] / $account5[$i]['all']['battles']), 0, PHP_ROUND_HALF_UP);
                        $tank_rhm_Rbatl1 = $account5[$i]['all']['battles'];
                        $tank_rhm_Rsvet1=round(($account5[$i]['all']['spotted'] / $account5[$i]['all']['battles']), 0, PHP_ROUND_HALF_UP);
                        $tank_rhm_Rvighil = $account5[$i]['all']['survived_battles'];
                        $tank_rhm_Ruron = $account5[$i]['all']['damage_dealt'];
                        $tank_rhm_Rsvet=$account5[$i]['all']['spotted'];
                    }
                    if($account5[$i]['stronghold_defense']['battles']>30) {
                        $tank_rhm_Nvighil = $account5[$i]['stronghold_defense']['survived_battles'];
                        $tank_rhm_Nuron = $account5[$i]['stronghold_defense']['damage_dealt'];
                        $tank_rhm_Nbatl = $account5[$i]['stronghold_defense']['battles'];
                        $tank_rhm_Nsvet = $account5[$i]['stronghold_defense']['spotted'];
