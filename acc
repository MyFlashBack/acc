                        $tank_Bat155_Ruron1 = round(($account5[$i]['all']['damage_dealt'] / $account5[$i]['all']['battles']), 0, PHP_ROUND_HALF_UP);
                        $tank_Bat155_Rbatl1 = $account5[$i]['all']['battles'];
                        $tank_Bat155_Ruron = $account5[$i]['all']['damage_dealt'];
                        $tank_Bat155_Rbatl = $account5[$i]['all']['battles'];
                    }
                    if($account5[$i]['stronghold_defense']['battles']>30) {
                        $tank_Bat155_Nuron = $account5[$i]['stronghold_defense']['damage_dealt'] ;
                        $tank_Bat155_Nbatl = $account5[$i]['stronghold_defense']['battles'];
                    }
                    if($account5[$i]['stronghold_skirmish']['battles']>30) {
                        $tank_Bat155_Yuron = $account5[$i]['stronghold_skirmish']['damage_dealt'];
                        $tank_Bat155_Ybatl = $account5[$i]['stronghold_skirmish']['battles'];
                    }
                    if($account5[$i]['stronghold_defense']['battles']>30 || $account5[$i]['stronghold_skirmish']['battles']>30) {
                        $tank_Bat155_NYbatl = $tank_Bat155_Nbatl+ $tank_Bat155_Ybatl;
                        $tank_Bat155_NYuron =  round((($tank_Bat155_Nuron+$tank_Bat155_Yuron )/ $tank_Bat155_NYbatl),0,PHP_ROUND_HALF_UP);
                    }
                }
                if($account5[$i]['tank_id']==$tank_Conq) {
                    if($account5[$i]['all']['battles']>30) {
                        $tank_Conq_Ruron1 = round(($account5[$i]['all']['damage_dealt'] / $account5[$i]['all']['battles']), 0, PHP_ROUND_HALF_UP);
                        $tank_Conq_Rbatl1 = $account5[$i]['all']['battles'];
                        $tank_Conq_Ruron = $account5[$i]['all']['damage_dealt'];
                        $tank_Conq_Rbatl = $account5[$i]['all']['battles'];
                    }
                    if($account5[$i]['stronghold_defense']['battles']>30) {
                        $tank_Conq_Nuron = $account5[$i]['stronghold_defense']['damage_dealt'] ;
                        $tank_Conq_Nbatl = $account5[$i]['stronghold_defense']['battles'];
                    }
                    if($account5[$i]['stronghold_skirmish']['battles']>30) {
                        $tank_Conq_Yuron = $account5[$i]['stronghold_skirmish']['damage_dealt'];
                        $tank_Conq_Ybatl = $account5[$i]['stronghold_skirmish']['battles'];
                    }
                    if($account5[$i]['stronghold_defense']['battles']>30 || $account5[$i]['stronghold_skirmish']['battles']>30) {
                        $tank_Conq_NYbatl = $tank_Conq_Nbatl+ $tank_Conq_Ybatl;
                        $tank_Conq_NYuron =  round((($tank_Conq_Nuron+$tank_Conq_Yuron) / $tank_Conq_NYbatl),0,PHP_ROUND_HALF_UP);
                    }
                }
            }
            if($account5[$i]['tank_id']==0)
                break;
        }
    }
